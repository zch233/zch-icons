{
    "name": "gupo-icons-base",
    "version": "0.0.1",
    "sideEffects": false,
    "author": "zhangchao <529743595@qq.com>",
    "license": "MIT",
    "main": "lib/index.js",
    "module": "es/index.js",
    "files": [
        "lib",
        "es",
        "inline-svg",
        "inline-namespaced-svg",
        "scripts/digest.json"
    ],
    "scripts": {
        "generate": "cross-env NODE_ENV=production gulp --require ts-node/register/transpile-only",
        "type-check": "tsc --noEmit",
        "build:es": "tsc --project tsconfig.build.json --module esnext --outDir es",
        "build:lib": "tsc --project tsconfig.build.json --module commonjs --outDir lib",
        "build": "cross-env NODE_ENV=production && npm run build:es && npm run build:lib",
        "lint:prettier": "prettier --write . --loglevel warn --ignore-unknown",
        "pre-commit:pretty": "pretty-quick --staged",
        "test": "ts-jest",
        "prepublishOnly": "yarn generate && yarn build",
        "digest": "node scripts/digest.mjs"
    },
    "devDependencies": {
        "@rgrove/parse-xml": "3.0.0",
        "@types/gulp": "4.0.9",
        "@types/gulp-concat": "0.0.33",
        "@types/gulp-header": "2.0.1",
        "@types/gulp-rename": "2.0.1",
        "@types/jest": "^28.1.1",
        "@types/lodash.camelcase": "4.3.7",
        "@types/lodash.clonedeep": "4.5.7",
        "@types/lodash.template": "4.5.1",
        "@types/node": "17.0.35",
        "@types/svgo": "2.6.3",
        "@types/through2": "2.0.36",
        "cross-env": "7.0.3",
        "del": "^6.1.1",
        "globby": "^13.1.2",
        "gulp": "4.0.2",
        "gulp-concat": "2.6.1",
        "gulp-header": "2.0.9",
        "gulp-rename": "2.0.0",
        "husky": "^7.0.0",
        "jest": "^28.1.1",
        "lodash.camelcase": "4.3.0",
        "lodash.clonedeep": "4.5.0",
        "lodash.template": "4.5.0",
        "prettier": "2.6.2",
        "pretty-quick": "3.1.3",
        "svgo": "2.8.0",
        "through2": "4.0.2",
        "ts-jest": "^28.0.5",
        "ts-node": "10.7.0",
        "typescript": "4.6.4"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "dependencies": {}
}
